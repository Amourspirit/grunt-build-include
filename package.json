{
  "name": "grunt-build-include",
  "version": "0.9.0",
  "description": "Grunt task to replace BUILD_INCLIDE with options statments usng grunt",
  "main": "./task/build_include.js",
  "types": "./task/build_include.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "gruntplugin",
    "build",
    "build-replace",
    "build-include",
    "replace",
    "include",
    "task"
  ],
  "author": "Paul Moss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amourspirit/grunt-build-include.git"
  },
  "files": [
    "tasks"
  ],
  "devDependencies": {
    "@ephox/tslint-rules": "^1.0.6",
    "@types/chai": "^4.1.7",
    "@types/grunt": "^0.4.25",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-nodeunit": "^2.0.0",
    "grunt-env": "^0.4.4",
    "grunt-remove-comments": "^0.1.6",
    "grunt-shell": "^3.0.1",
    "grunt-tslint": "^5.0.2",
    "load-grunt-tasks": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.2",
    "mocha-sinon": "^2.1.0",
    "prettier": "^1.16.4",
    "sinon": "^7.3.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typedoc-plugin-markdown": "^1.2.0",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "file-sync-cmp": "^0.1.1",
    "multi-encoder": "^0.4.3",
    "string-breaker": "^0.4.1"
  },
  "engines": {
    "node": ">=6"
  }
}
